			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "execute population transformation",
						"type": "DatabricksNotebook",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/covid/set-up/mount_storage"
						},
						"linkedServiceName": {
							"referenceName": "ls_db_covid",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "execute population tranformation",
						"type": "DatabricksNotebook",
						"dependsOn": [
							{
								"activity": "execute population transformation",
								"dependencyConditions": [
