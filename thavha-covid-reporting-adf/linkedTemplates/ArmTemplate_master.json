{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "thavha-covid-reporting-adf"
		},
		"ls_ablob_coovidanalysissa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_coovidanalysissa'"
		},
		"ls_adls_covidanalysislake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidanalysislake'"
		},
		"ls_adls_covidanalysislake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidanalysislake.dfs.core.windows.net/"
		},
		"tr_population_data_triger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e73288a2-b293-4973-8bff-03cbacf8dec6/resourceGroups/rg-covid-19/providers/Microsoft.Storage/storageAccounts/coovidanalysissa"
		},
		"ls_http_opendata_ecde_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"tr_ingest_github_data_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/blob/main/ecdc_data/case_deaths_uk_ind_only.csv"
		},
		"tr_ingest_github_data_properties_pl_ingest_ecdc_data_parameters_sinkfileName": {
			"type": "string",
			"defaultValue": "case_deaths_uk_ind_only.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "thavha-covid-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_coovidanalysissa_connectionString": {
						"value": "[parameters('ls_ablob_coovidanalysissa_connectionString')]"
					},
					"ls_adls_covidanalysislake_accountKey": {
						"value": "[parameters('ls_adls_covidanalysislake_accountKey')]"
					},
					"ls_adls_covidanalysislake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidanalysislake_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecde_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecde_europa_eu_properties_typeProperties_url')]"
					},
					"tr_population_data_triger_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_triger_properties_typeProperties_scope')]"
					},
					"tr_ingest_github_data_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL": {
						"value": "[parameters('tr_ingest_github_data_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL')]"
					},
					"tr_ingest_github_data_properties_pl_ingest_ecdc_data_parameters_sinkfileName": {
						"value": "[parameters('tr_ingest_github_data_properties_pl_ingest_ecdc_data_parameters_sinkfileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}